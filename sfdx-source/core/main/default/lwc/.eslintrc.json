{
  "extends": [
    "@salesforce/eslint-config-lwc/recommended"
  ],
  "overrides": [
    {
      "files": [
        "*.test.js"
      ],
      "rules": {
        "@lwc/lwc/no-unexpected-wire-adapter-usages": "off"
      },
      "env": {
        "node": true
      }
    }
  ],
  "rules": {
    "no-unused-vars": "off",
    "@lwc/lwc/no-api-reassignments": "off"
  }
}
